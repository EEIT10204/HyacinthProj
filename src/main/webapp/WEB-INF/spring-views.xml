<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="login.success" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true"></property>
		<property name="url" value="/Index/Index.jsp"></property>
	</bean>

	<bean id="profileSelf.page" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true"></property>
		<property name="url" value="/Member/ProfileSelf.jsp"></property>
	</bean>
		
	<bean id="verifySuccess.page" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/Member/Success.jsp"></property>
	</bean>
	
	<bean id="verifyError.page" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true"></property>
		<property name="url" value="/Index/Index.jsp"></property>
	</bean>
	
	<bean id="forwardProfileMember" class="org.springframework.web.servlet.view.InternalResourceView">
 	 	<property name="url" value="/Member/ProfileMember.jsp"></property>
 	</bean>
 	
	 <bean id="forwardProfileSelf" class="org.springframework.web.servlet.view.InternalResourceView">
  		<property name="url" value="/Member/ProfileSelf.jsp"></property>
 	</bean>
 
 	<bean id="redirectIndex" class="org.springframework.web.servlet.view.RedirectView">
  		<property name="contextRelative" value="true"></property>
  		<property name="url" value="/Index/Index.jsp"></property>
 	</bean>
 	<bean id="forwardindex" class="org.springframework.web.servlet.view.InternalResourceView">
 		 <property name="url" value="/Index/Index.jsp"></property>
 	</bean>
	
	
</beans>
